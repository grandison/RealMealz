CREATE TABLE "appliances" ("id" INTEGER PRIMARY KEY AUTOINCREMENT NOT NULL, "name" varchar(255));
CREATE TABLE "appliances_kitchens" ("id" INTEGER PRIMARY KEY AUTOINCREMENT NOT NULL, "appliance_id" integer, "kitchen_id" integer);
CREATE TABLE "branches" ("id" INTEGER PRIMARY KEY AUTOINCREMENT NOT NULL, "name" varchar(255), "street" varchar(255), "city" varchar(255), "state" varchar(255), "zip" varchar(255), "phone" varchar(255), "website" varchar(255), "email" varchar(255), "store_id" integer);
CREATE TABLE "branches_products" ("id" INTEGER PRIMARY KEY AUTOINCREMENT NOT NULL, "product_id" integer, "branch_id" integer, "on_hand" integer, "location" varchar(255));
CREATE TABLE "branches_users" ("id" INTEGER PRIMARY KEY AUTOINCREMENT NOT NULL, "distance" float, "preference" integer, "user_id" integer, "branch_id" integer);
CREATE TABLE "categories" ("id" INTEGER PRIMARY KEY AUTOINCREMENT NOT NULL, "name" varchar(255));
