CREATE TABLE "courses" ("id" INTEGER PRIMARY KEY AUTOINCREMENT NOT NULL, "servings" float, "meal_id" integer, "recipe_id" integer, "is_eaten" boolean DEFAULT 'f', "order_num" integer DEFAULT 1);
CREATE TABLE "ingredients" ("id" INTEGER PRIMARY KEY AUTOINCREMENT NOT NULL, "name" varchar(255), "description" varchar(255), "recipe_id" integer, "allergen1_id" integer, "allergen2_id" integer, "allergen3_id" integer);
